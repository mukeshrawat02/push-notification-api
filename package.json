{
  "name": "push_notification_api",
  "version": "1.0.0",
  "description": "Rest api for sending device notification and registration.",
  "main": "server.js",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "dev": "ts-node ./src/main.ts",
    "start": "node ./dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --detectOpenHandles --forceExit --config  ./test/jest-e2e.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "Mukesh Rawat",
  "license": "GPL",
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/mongoose": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@types/es6-promise": "^3.3.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "firebase-admin": "^6.0.0",
    "fs": "0.0.1-security",
    "http-errors": "^1.7.0",
    "http-status-codes": "^1.3.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.6",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^23.3.5",
    "@types/mongodb": "^3.1.3",
    "@types/mongoose": "^5.2.3",
    "@types/node": "^10.5.4",
    "@types/supertest": "^2.0.6",
    "jest": "^23.6.0",
    "nock": "^10.0.1",
    "nodemon": "^1.18.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0"
  }
}